{
    "manifest_version": 2,
    "name": "Lieferando-Filter",
    "description": "Filtert die Ergebnisse von Restaurants auf Lieferando",
    "version": "1.1",
    "icons": {
    "128": "128.png"
    },
    "browser_action": {
        "default_title": "Lieferando-Filter",
        "default_popup": "main.html",
        "default_icon": "128.png"
    },
    "content_scripts": [{
        
        "js": ["content.js"],
        "matches": ["https://www.lieferando.de/*"]
    }],
    "permissions": [
    "unlimitedStorage",
    "notifications",
    "storage"
    ]
    }